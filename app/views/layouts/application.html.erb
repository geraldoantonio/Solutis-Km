<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
  
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
  
   <!-- custom -->
    <%= stylesheet_link_tag    'application', media: 'all' %> 
    <!-- Font Aewsome -->
    <%= stylesheet_link_tag 'https://use.fontawesome.com/releases/v5.2.0/css/all.css', {integrity: 'sha384-hWVjflwFxL6sNzntih27bfxkr27PmbbK/iSvJ+a4+0owXq79v+lsFkW54bOGbiDQ', crossorigin:'anonymous'} %>   
    <!-- BootStrap 4 -->
    <%= stylesheet_link_tag 'https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css', {integrity:'sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm', crossorigin:'anonymous'} %>
    <!-- Toastr -->
    <%= stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css' %>   

  
    <!-- Custom -->
    <%= javascript_include_tag 'application' %>
    <!-- Fonte Awesomw -->
    <%= javascript_include_tag 'https://use.fontawesome.com/releases/v5.2.0/js/all.js', {integrity: 'sha384-4oV5EgaV02iISL2ban6c/RmotsABqE4yZxZLcYMAdG7FAPsyHYAPpywE9PJo+Khy', crossorigin:'anonymous'} %>
    <!-- Bootstrap -->
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js', {integrity:'sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q', crossorigin:'anonymous'}%>
    <%= javascript_include_tag 'https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js', {integrity:'sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl', crossorigin:'anonymous'} %>
       <!-- Toastr -->
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js' %>

    
    
  <title>Solutis - Suporte N2 </title>

  </head>
  <body >   
    <header >
      <!-- Menu -->
      <%= render partial:"layouts/navigation" if user_signed_in? %>      
    </header>
    
    <!-- Begin page content -->
    <main role="main" class="container " style="margin-top:60px;">     
      <%= yield %>      
    </main>
    <footer class="footer">      
      <%= render partial:"layouts/footer" %> 
    </footer>    
             
    <%= render partial: 'layouts/notifications' %>
  </body>
  
</html>










